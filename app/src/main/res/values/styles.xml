    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/text_title_color</item>
        <!--状态栏-->
        <item name="colorPrimaryDark">@color/gray</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="colorAccent">@color/text_title_color</item>
    </style>

    <style name="AppTheme_Splict" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@mipmap/default_start</item>
        <item name="android:textColor">@color/text_title_color</item>
        <!--状态栏-->
        <item name="colorPrimaryDark">@color/gray</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="colorAccent">@color/text_title_color</item>
    </style>

